# This file uses the jl_lib make commands.

build:
	# Run jl-lib's Makefile
	make -f $(shell echo $(JLL_HOME))/Makefile build-project --silent
run:
	./build/bin/`sed -n '4p' data.txt`
android:
	sh $(shell echo $(JLL_HOME))/compile-scripts/jl_android\
	 $(shell echo $(JLL_HOME))
install:
	make -f $(shell echo $(JLL_HOME))/Makefile install-project --silent
